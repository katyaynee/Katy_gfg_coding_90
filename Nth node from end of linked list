
class Solution
{
    //Function to find the data of nth node from the end of a linked list.
    int getNthFromLast(Node head, int n)
    {
    	// Your code here
    	
    	Node slow=head;
    	Node fast=head;
    	
    	int c=0;
    	for(int i=0;i<n;i++)
    	{
    	    if (fast==null)
    	    return -1;
    	    fast=fast.next;
    	  
    	}
    	
        if(fast==null)
        return head.data;
    	
    	while(fast.next!=null)
    	{
    	    slow=slow.next;
    	    fast=fast.next;
    	}
    	return slow.next.data;
    	
    }
}
